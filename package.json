    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-storysource": "^4.0.4",
